<%= render :partial => 'admin/shared/configuration_menu' %>

<h1>Edit Black List</h1>
<%= render "shared/error_messages", :target => @black_list %>
<%= form_for(@black_list, :url => object_url, :html => { :method => :put }) do |f| %>
    <%= render :partial => "form", :locals => { :f => f } %>
    <p class="form-buttons">
      <%= button t('update') %>
      <%= t("or") %> <%= link_to t("actions.cancel"), admin_black_lists_url %>
    </p>
<% end %>
