<IfModule mod_rewrite.c>
RewriteEngine On
RewriteBase /
RewriteCond %{HTTP_HOST} ^thefootballshirt.com [NC]
RewriteRule ^(.*)$ http://www.thefootballshirt.com/$1 [L,R=301]
</IfModule>
<IfModule mod_rewrite.c>
